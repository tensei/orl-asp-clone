@using System.Collections.Generic
@{
    ViewBag.Title = "Days";
}

<ul class="collection with-header">
    @foreach (var ch in (List<string>)ViewData["days"])
    {
        <a class="collection-item" href="/@ViewData["channel"]/@ViewData["month"]/@ch">
            <i class="fa fa-file fa-fw" aria-hidden="true"></i><span>@ch</span>
        </a>
    }
</ul>
